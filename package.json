{
  "name": "arm-server",
  "author": "BeeeQueue <adam.lj.haglund@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "engines": {
    "node": ">=16.15"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "tsx bin/update.ts --exit",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "yarn run migrate && tsx src/index.ts",
    "test": "cross-env NODE_ENV=test yarn run migrate && yarn vitest run --coverage",
    "test:dev": "cross-env NODE_ENV=test yarn run migrate && yarn vitest",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/node": "16.11.41"
  },
  "dependencies": {
    "@sentry/node": "7.2.0",
    "cross-env": "7.0.3",
    "envsafe": "2.0.3",
    "fastify": "3.27.4",
    "fastify-cors": "6.0.3",
    "fastify-helmet": "7.0.1",
    "got": "11.8.3",
    "knex": "2.1.0",
    "nanoid": "3.3.1",
    "pino": "8.0.0",
    "pino-logflare": "0.3.12",
    "pino-pretty": "8.0.0",
    "sqlite3": "5.0.7",
    "ts-json-validator": "0.7.1",
    "tsx": "3.4.3",
    "type-fest": "2.12.0",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.6.0",
    "@tsconfig/node16-strictest": "1.0.2",
    "@types/node": "16.11.41",
    "c8": "7.11.0",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "sucrase": "3.21.0",
    "tsconfig-paths": "4.0.0",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.6.1"
  }
}
