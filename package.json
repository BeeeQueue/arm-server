{
  "name": "arm-server",
  "author": "BeeeQueue <adam@haglund.dev>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "packageManager": "pnpm@7.14.0",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "",
    "docker:build": "docker build . --tag arm-server",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name arm -p 3000:3000 arm-server",
    "fetch-data": "tsx bin/update.ts --exit",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "pnpm run migrate && tsx src/index.ts",
    "test": "pnpm run migrate && pnpm vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/node": "18.11.5",
    "node-fetch": "npm:node-fetch-native@0.1.8"
  },
  "dependencies": {
    "@fastify/cors": "8.1.0",
    "@fastify/helmet": "10.0.2",
    "@sentry/node": "7.16.0",
    "better-sqlite3": "7.6.2",
    "fastify": "4.9.2",
    "knex": "2.3.0",
    "nanoid": "4.0.0",
    "ohmyfetch": "0.4.20",
    "pino": "8.7.0",
    "pino-pretty": "9.1.1",
    "tsx": "3.10.4",
    "type-fest": "3.1.0",
    "typescript": "4.8.4",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.0.2",
    "@tsconfig/node16-strictest": "1.0.4",
    "@types/json-schema": "7.0.11",
    "@types/node": "18.11.5",
    "@vitest/coverage-c8": "0.24.3",
    "ajv": "8.11.0",
    "dotenv": "16.0.3",
    "eslint": "8.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sucrase": "3.28.0",
    "tsconfig-paths": "4.1.0",
    "vitest": "0.24.3"
  }
}
