{
  "name": "arm-server",
  "author": "BeeeQueue <adam.lj.haglund@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "engines": {
    "node": "^16"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "ts-node bin/update.ts",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "yarn run migrate && ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test yarn run migrate && yarn jest --forceExit",
    "postinstall": "husky install"
  },
  "resolutions": {
    "@types/node": "16.11.11"
  },
  "dependencies": {
    "@opentelemetry/api": "1.0.3",
    "@sentry/node": "6.15.0",
    "cross-env": "7.0.3",
    "envsafe": "2.0.3",
    "fastify": "3.24.1",
    "fastify-cors": "6.0.2",
    "fastify-helmet": "5.3.2",
    "got": "11.8.2",
    "knex": "0.95.14",
    "lightstep-opentelemetry-launcher-node": "0.24.0",
    "nanoid": "3.1.30",
    "pino": "7.5.1",
    "pino-logflare": "0.3.12",
    "pino-pretty": "7.2.0",
    "sqlite3": "5.0.2",
    "ts-json-validator": "0.7.1",
    "ts-node": "10.4.0",
    "type-fest": "2.5.3",
    "typescript": "4.5.0-dev.20211101"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.4.0",
    "@tsconfig/node14": "1.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "12.1.2",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.11.0"
  }
}
