{
  "name": "arm-server",
  "author": "BeeeQueue <adam@haglund.dev>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "tsx bin/update.ts --exit",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "yarn run migrate && tsx src/index.ts",
    "test": "cross-env NODE_ENV=test yarn run migrate && yarn vitest run --coverage",
    "test:dev": "cross-env NODE_ENV=test yarn run migrate && yarn vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/node": "18.7.14",
    "node-fetch": "npm:node-fetch-native@0.1.4"
  },
  "dependencies": {
    "@fastify/cors": "8.1.0",
    "@fastify/helmet": "10.0.0",
    "@sentry/node": "7.12.1",
    "better-sqlite3": "7.6.2",
    "cross-env": "7.0.3",
    "fastify": "4.5.3",
    "knex": "2.3.0",
    "nanoid": "4.0.0",
    "ohmyfetch": "0.4.18",
    "pino": "8.5.0",
    "pino-pretty": "9.1.0",
    "tsx": "3.9.0",
    "type-fest": "2.19.0",
    "typescript": "4.8.2",
    "zod": "3.18.0"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.8.0",
    "@tsconfig/node16-strictest": "1.0.3",
    "@types/json-schema": "7.0.11",
    "@types/node": "18.7.14",
    "@vitest/coverage-c8": "0.23.0",
    "ajv": "8.11.0",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "sucrase": "3.25.0",
    "tsconfig-paths": "4.1.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.0"
  }
}
