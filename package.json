    "ts-node": "8.10.2",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/supertest": "2.0.8",
    "dotenv": "8.2.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-node-dev": "1.0.0-pre.49",
