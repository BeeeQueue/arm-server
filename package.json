{
  "name": "arm-server",
  "author": "BeeeQueue <adam.lj.haglund@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "tsx bin/update.ts --exit",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "yarn run migrate && tsx src/index.ts",
    "test": "cross-env NODE_ENV=test yarn run migrate && yarn vitest run --coverage",
    "test:dev": "cross-env NODE_ENV=test yarn run migrate && yarn vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/node": "18.0.0",
    "node-fetch": "npm:node-fetch-native@0.1.3"
  },
  "dependencies": {
    "@fastify/cors": "8.0.0",
    "@fastify/helmet": "9.1.0",
    "@sentry/node": "7.2.0",
    "cross-env": "7.0.3",
    "fastify": "4.0.3",
    "knex": "2.1.0",
    "nanoid": "4.0.0",
    "ohmyfetch": "0.4.18",
    "pino": "8.0.0",
    "pino-pretty": "8.0.0",
    "sqlite3": "5.0.8",
    "tsx": "3.4.3",
    "type-fest": "2.13.1",
    "typescript": "4.7.4",
    "zod": "3.17.3"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.6.0",
    "@tsconfig/node16-strictest": "1.0.2",
    "@types/json-schema": "7.0.11",
    "@types/node": "18.0.0",
    "ajv": "8.11.0",
    "c8": "7.11.3",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "prettier": "2.7.1",
    "sucrase": "3.21.0",
    "tsconfig-paths": "4.0.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.15.2"
  }
}
