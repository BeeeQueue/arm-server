    "@types/koa": "2.0.49",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-router": "7.0.42",
    "@types/superagent": "4.1.3",
    "joi": "14.3.1",
    "knex": "0.19.4",
    "koa": "2.8.2",
    "koa-bodyparser": "4.2.1",
    "koa-router": "7.4.0",
    "pg": "7.12.1",
    "sqlite3": "4.1.0",
    "superagent": "5.1.0",
    "ts-node": "8.3.0",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/supertest": "2.0.8",
    "dotenv": "8.1.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node-dev": "1.0.0-pre.42",
    "tsconfig-paths": "3.8.0"
  },
  "license": "AGPL-3.0-only",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "ts-node bin/update.ts",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "prestart": "yarn run migrate",
    "pretest": "NODE_ENV=test yarn run migrate",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "NODE_ENV=test yarn jest"
  }
}
