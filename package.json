{
  "name": "arm-server",
  "author": "BeeeQueue <adam.lj.haglund@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "engines": {
    "node": ">=14.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "fetch-data": "ts-node bin/update.ts",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "yarn run migrate && ts-node -r tsconfig-paths/register src/index.ts",
    "test": "cross-env NODE_ENV=test yarn run migrate && yarn jest"
  },
  "resolutions": {
    "@types/node": "14.17.2"
  },
  "dependencies": {
    "@sentry/node": "6.3.6",
    "@sentry/tracing": "6.3.6",
    "cross-env": "7.0.3",
    "envsafe": "2.0.3",
    "got": "11.8.2",
    "joi": "17.4.0",
    "knex": "0.21.15",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-connect": "2.1.0",
    "koa-pino-logger": "3.0.0",
    "koa-router": "10.0.0",
    "pino": "6.11.3",
    "pino-pretty": "4.8.0",
    "sqlite3": "5.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.1.3",
    "@tsconfig/node14": "1.0.0",
    "@types/jest": "26.0.23",
    "@types/koa": "2.13.1",
    "@types/koa-bodyparser": "4.3.0",
    "@types/koa-pino-logger": "3.0.0",
    "@types/koa-router": "7.4.2",
    "@types/node": "14.17.2",
    "@types/supertest": "2.0.11",
    "dotenv": "9.0.2",
    "eslint": "7.26.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0"
  }
}
