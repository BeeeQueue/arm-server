{
  "name": "arm-server",
  "author": "BeeeQueue <adam@haglund.dev>",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/BeeeQueue/arm-server",
  "packageManager": "pnpm@7.18.2",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "",
    "docker:build": "docker build . --tag arm-server",
    "docker:prune": "docker image prune --force",
    "docker:run": "docker run --rm --name arm -p 3000:3000 arm-server",
    "fetch-data": "tsx bin/update.ts --exit",
    "lint": "eslint src",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:create": "knex migrate:make --knexfile knexfile.ts -x ts",
    "start": "pnpm run migrate && tsx src/index.ts",
    "test": "pnpm run migrate && pnpm vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "resolutions": {
    "@types/node": "18.11.18",
    "node-fetch": "npm:node-fetch-native@1.0.1"
  },
  "dependencies": {
    "@fastify/cors": "8.2.0",
    "@fastify/helmet": "10.1.0",
    "@sentry/node": "7.28.0",
    "better-sqlite3": "8.0.1",
    "fastify": "4.10.2",
    "knex": "2.3.0",
    "nanoid": "4.0.0",
    "ohmyfetch": "0.4.21",
    "pino": "8.8.0",
    "pino-pretty": "9.1.1",
    "tsx": "3.12.1",
    "type-fest": "3.4.0",
    "typescript": "4.9.4",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "1.1.2",
    "@tsconfig/node16-strictest": "1.0.4",
    "@types/json-schema": "7.0.11",
    "@types/node": "18.11.18",
    "@vitest/coverage-c8": "0.26.3",
    "ajv": "8.11.2",
    "dotenv": "16.0.3",
    "eslint": "8.31.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "sucrase": "3.29.0",
    "tsconfig-paths": "4.1.1",
    "vitest": "0.26.3"
  }
}
